# Remote Commander Configuration
# Copy this file to .env and fill in your values

# Server URL (use localhost for development)
REMOTE_COMMANDER_SERVER_URL=http://localhost:8000

# Authentication credentials
REMOTE_COMMANDER_USERNAME=your-username
REMOTE_COMMANDER_PASSWORD=your-password

# Base path for file operations (optional, defaults to /data)
REMOTE_COMMANDER_BASE_PATH=/data
